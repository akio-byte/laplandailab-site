{
  "name": "laplandailab-site",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --max-warnings=0",
    "test": "vitest",
    "test:run": "vitest run",
    "check:deps": "depcheck --ignores=@types/node,@types/react,@types/react-dom,autoprefixer,postcss,tailwindcss,axe-core",
    "check:audit": "npm audit --audit-level=high",
    "check:ci": "npm run lint && npm run test:run && npm run check:deps && npm run check:audit"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@netlify/functions": "^4.2.6",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.10.3",
    "depcheck": "^1.4.7",
    "eslint": "^9.36.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^3.2.4",
    "vitest-axe": "^0.1.0"
  }
}
